<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ea.mpp.library.views.LibrerianController">
   <children>
      <TabPane layoutY="40.0" prefHeight="558.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Checkin">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
          <Tab text="Checkout">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label layoutX="67.0" layoutY="58.0" text="Member ID" />
                        <Label layoutX="69.0" layoutY="96.0" text="Book ISBN" />
                        <TextField fx:id="memberId" layoutX="151.0" layoutY="54.0" />
                        <TextField fx:id="isbn" layoutX="151.0" layoutY="92.0" />
                        <Button layoutX="265.0" layoutY="130.0" mnemonicParsing="false" onAction="#onBookGetAction" text="Get" />
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab text="Search Book">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
          
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
